{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "media/**",
          "_themes/**",
		  "ex_resource/**"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/DocPacker/**"
        ]
      }
    ],
    "globalMetadata": {
      "breadcrumb_path": "/EM/toc.json",
      "ROBOTS": "NOINDEX,NOFOLLOW"
    },
    "template": [
      "docs.html"
    ],
    "dest": "articles/multi-factor-authentication"
  }
}
{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "media/**",
          "_themes/**",
          "ex_resource/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/DocPacker/**"
        ]
      }
    ],
    "globalMetadata": {
      "breadcrumb_path": "/enterprise-mobility/toc.json",
      "ROBOTS": "NOINDEX,NOFOLLOW"
    },
    "template": ["docs.html"],
    "dest": "articles/active-directory"
  }
}

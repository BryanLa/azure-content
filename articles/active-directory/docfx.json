{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "media/**",
          "_themes/**",
		  "ex_resource/**"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/DocPacker/**"
        ]
      }
    ],
    "template": [
      "docs.html"
    ],
    "dest": "articles/active-directory"
  }
}